{
  "name": "mobx-reactive",
  "private": false,
  "version": "0.0.0",
  "description": "MobX reactivity system with power of RxJS declarative operators",
  "author": "wintpann",
  "keywords": [
    "RxJS",
    "mobx",
    "reactive",
    "functional",
    "signal"
  ],
  "homepage": "https://github.com/wintpann/mobx-reactive",
  "repository": {
    "type": "git",
    "url": "https://github.com/wintpann/mobx-reactive.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wintpann/mobx-reactive/issues",
    "email": "vvintpann@gmail.com"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --port=5000",
    "test": "jest",
    "make": "npx cross-env NODE_ENV=production rollup -c && dts-bundle-generator -o dist/index.d.ts src/core/index.ts",
    "lint": "tsc --noEmit && eslint src --fix --ext ts,tsx --max-warnings 0",
    "prepare": "husky install",
    "build": "yarn lint && yarn test && rimraf dist && yarn make",
    "release": "yarn build && npx np"
  },
  "peerDependencies": {
    "mobx": ">=6.0.2"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@rollup/plugin-url": "^8.0.1",
    "@types/jest": "^29.5.1",
    "@types/lodash-es": "^4.17.7",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-plugin-jsx-wrapper": "^1.0.0",
    "cross-env": "^7.0.3",
    "dts-bundle-generator": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": ">=6",
    "jest": "^29.5.0",
    "lint-staged": ">=10",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "np": "^8.0.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.0",
    "rollup-plugin-terser": "^7.0.2",
    "storybox-react": "^1.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
